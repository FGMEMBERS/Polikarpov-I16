<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <fx>
    <crank>
     <name>engstart</name>
     <path>Sounds/engstart.wav</path>
     <condition>
       <property>/engines/engine[0]/cranking</property>
     </condition>
     <position>
      <x>15.0</x>
      <y>-5.0</y>
      <z>0.0</z>
     </position>
     <volume>
      <offset>0.4</offset>
     </volume>
    </crank>

    <crank>
     <name>crank1</name>
     <mode>looped</mode>
     <path>Sounds/cranking.wav</path>
     <condition>
       <and>
        <property>/engines/engine[0]/cranking</property>
        <not>
         <property>/engines/engine[0]/running</property>
        </not>
       </and>
     </condition>
     <position>
      <x>15.0</x>
      <y>-5.0</y>
      <z>0.0</z>
     </position>
     <volume>
      <offset>0.5</offset>
     </volume>
    </crank>

    <crank> 
      <name>cough</name>
      <path>Sounds/cranking.wav</path>
      <property>/engines/engine/running</property>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <engine> 
      <name>engineslowrun</name>
      <mode>looped</mode>
      <path>Aircraft/Polikarpov-I16/Sounds/low.wav</path>
      <condition> 
        <property>/engines/engine/running</property>
        <less-than> 
          <property>/engines/engine/rpm</property>
          <value> 1600 </value>
        </less-than>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min> 1.0 </min>
        <max> 1.5 </max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>  1.0 </offset>
        <factor> -0.4 </factor>
      </volume>
      <pitch> 
        <property>/engines/engine/rpm</property>
        <factor> 0.0006 </factor>
        <min> 0.4 </min>
        <max> 3.0 </max>
        <offset> 0 </offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine>
      <name>enginerun</name>
      <mode>looped</mode>
      <path>Aircraft/Polikarpov-I16/Sounds/middle.wav</path>
      <condition>
        <property>/engines/engine/running</property>
        <greater-than-equals> 
          <property>/engines/engine/rpm</property>
          <value> 1500 </value>
        </greater-than-equals>
      </condition>
      <volume>
        <property>/controls/engines/engine/throttle</property>
        <min> 0.5 </min>
        <max> 1.0 </max>
      </volume>
      <pitch>
        <property>/engines/engine/rpm</property>
        <factor> 0.00033 </factor>
        <min> 0.0 </min>
        <max> 1.0 </max>
        <offset> 0.0 </offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <condition>
        <and>
          <or>
            <property>/gear/gear[0]/wow</property>
            <property>/gear/gear[1]/wow</property>
          </or>
          <property>/gear/gear[0]/ground-is-solid</property>
        </and>
      </condition>
      <volume>
        <property>/velocities/groundspeed-kt</property>
        <factor> 0.4 </factor>
      </volume>
      <position>
        <x> -1.0 </x>
        <y>  0.0 </y>
        <z> -2.0 </z>
      </position>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rumble>

    <splash>
      <name>splash</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <condition>
        <and>
          <or>
            <property>/gear/gear[0]/wow</property>
            <property>/gear/gear[1]/wow</property>
          </or>
          <not>
            <property>/gear/gear[0]/ground-is-solid</property>
          </not>
        </and>
      </condition>
      <volume>
        <property>/velocities/groundspeed-kt</property>
        <offset> 0.00 </offset>
        <factor> 0.02 </factor>
        <min> 0.0 </min>
        <max> 0.5 </max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor> 0.01 </factor>
        <offset> 1.20 </offset>
        <max> 1.5 </max> 
      </pitch>
      <position>
        <x> -1.0 </x>
        <y>  0.0 </y>
        <z> -2.0 </z>
      </position>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </splash>

    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor> 0.5 </factor>
        <max> 1.0 </max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor> 0.01 </factor>
        <max> 1.0 </max>
      </volume>
      <volume>
        <internal>dt_stop</internal>
        <factor> 0.05 </factor>
        <max> 1.0 </max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor> 0.0025 </factor>
        <offset> 0.5000 </offset>
        <max> 1.0 </max>
      </pitch>
      <position>
        <x> -1.0 </x>
        <y>  0.0 </y>
        <z> -2.0 </z>
      </position>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>

    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/flap-pos-norm</property>
      <volume>
        <factor> 0.15 </factor>
      </volume>
      <position>
        <x> 1.0 </x>
        <y> 0.0 </y>
        <z> 1.0 </z>
      </position>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </flap>

    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>velocities/airspeed-kt</property>
      <volume>
        <property>position/altitude-ft</property>
        <factor> -0.000015 </factor>
        <offset>  1.000000 </offset>
        <min> 0.1 </min>
        <max> 1.0 </max>
      </volume>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor> 0.0015 </factor>
        <min> 0.03 </min>
        <max> 0.25 </max>
      </volume>
      <pitch>
        <property>velocities/airspeed-kt</property>
        <factor> 0.01 </factor>
        <offset> 1.00 </offset>
      </pitch>
      <position>
        <x> 0 </x>
        <y> 0 </y>
        <z> 0 </z>
      </position>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </wind>

    <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>Sounds/stall.wav</path>
      <property>sim/alarms/stall-warning</property>
      <volume>
        <property>/sim/alarms/stall-warning</property>
        <max> 1.0 </max>
      </volume>
      <position>
        <x> -0.5 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </stall>

    <!--                  Toggle Switches                  -->     
    <switch>
      <name>click on</name>
      <mode>once</mode>
      <path>Sounds/click.wav</path>
      <condition>
        <property>sim/sound/switch</property>
      </condition>
      <volume>
        <offset> 0.7 </offset>
      </volume>
      <position>
        <x> -0.5 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <reference-dist> 2.0 </reference-dist>
      <max-dist> 4.0 </max-dist>
    </switch>

    <switch>
      <name>click off</name>
      <mode>once</mode>
      <path>Sounds/click.wav</path>
      <condition>
        <not>
          <property>sim/sound/switch</property>
        </not>
      </condition>
      <volume>
        <offset> 0.7 </offset>
      </volume>
      <position>
        <x> -0.5 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <reference-dist> 2.0 </reference-dist>
      <max-dist> 4.0 </max-dist>
    </switch>

    <rain>
      <name>rain</name>
      <mode>looped</mode>
      <path>Aircraft/Polikarpov-I16/Sounds/rain_canopy.wav</path>
      <condition>
        <greater-than>
          <property>/environment/metar/rain-norm</property>
          <value>0.2</value>
        </greater-than>
      </condition>
      <volume>
        <property>/environment/metar/rain-norm</property>
        <factor>1.00</factor>
        <offset>0.00</offset>
        <min>0.4</min>
        <max>0.9</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.001</factor>
        <offset>1.0</offset>
      </pitch>
    </rain>

    <guns> 
      <name>gunfire</name>
      <mode>looped</mode>
      <path>Aircraft/Polikarpov-I16/Sounds/gunsfire.wav</path>
      <condition> 
        <property>controls/armament/trigger</property>
        <property>ai/submodels/submodel/count</property>
      </condition>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </guns>

    <gear>
      <name>gear</name>
      <mode>in-transit</mode>
      <path>Aircraft/Polikarpov-I16/Sounds/gears.wav</path>
      <property>gear/gear[0]/position-norm</property>
      <volume>
        <max>1</max>
      </volume>
    </gear>

    <waltz>
      <name>waltz</name>
      <mode>looped</mode>
      <path>Aircraft/Polikarpov-I16/Sounds/waltz.wav</path>
       <condition>
         <equals>
           <property>/instrumentation/comm[1]/frequencies/selected-mhz</property>
           <value>100.00</value>
         </equals>
       </condition>
        <volume>
          <max>0.7</max>
        </volume>
      </waltz>

    <crash>
      <name>intense ground contact</name>
      <mode>once</mode>
      <path>Aircraft/Polikarpov-I16/Sounds/crashed.wav</path>
      <property>/sim/crashed</property>
      <volume>
          <factor>1</factor>
      </volume>
      <pitch>
          <factor>1</factor>
      </pitch>
      <reference-dist>500</reference-dist>
      <max-dist>1000</max-dist>
    </crash>
  </fx>

</PropertyList>
