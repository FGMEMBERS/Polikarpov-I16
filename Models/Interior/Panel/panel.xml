      <x-m>  0.045 </x-m>
      <y-m> -0.070 </y-m>
      <z-m> -0.052 </z-m>
    </offsets>
  </model>

  <!-- Vsi -->
  <model>
    <path>Aircraft/Polikarpov-I16/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m>  0.045 </x-m>
      <y-m>  0.070 </y-m>
      <z-m> -0.052 </z-m>
    </offsets>
  </model>

</PropertyList>

