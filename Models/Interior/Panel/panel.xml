<?xml version="1.0"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- MagneticCompass -->
  <model>
    <path>Aircraft/Polikarpov-I16/Models/Interior/Panel/Instruments/MagneticCompass/compas.xml</path>
    <offsets>
      <x-m> 0.038 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.015 </z-m>
    </offsets>
  </model>

  <!-- Alt -->
  <model>
    <path>Aircraft/Polikarpov-I16/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.045 </x-m>
      <y-m> -0.080 </y-m>
      <z-m>  0.007 </z-m>
    </offsets>
  </model>

  <!-- Asi -->
  <model>
    <path>Aircraft/Polikarpov-I16/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m> 0.045 </x-m>
      <y-m> 0.080 </y-m>
      <z-m> 0.007 </z-m>
    </offsets>
  </model>

  <!-- Turn -->
